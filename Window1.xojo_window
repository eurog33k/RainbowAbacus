#tag WindowCode
	#tag Event
		Sub Open()
		  Redim ColumnTopValues(17)
		  Redim ColumnBottomValues(17)
		  Redim ColumnValues(17)
		  Dim strWidths() As String
		  
		  For iRow As Integer = 0 To 1
		    myListBox1.AddRow("")
		    For iColumn As Integer = 0 to myListbox1.ColumnCount - 1
		      if iRow = 0 then
		        myListBox1.celltag(myListbox1.LastIndex, iColumn) = 1
		      else
		        myListBox1.celltag(myListbox1.LastIndex, iColumn) = 0
		      end if
		    Next
		  Next
		  
		  For iRow As Integer = 0 To 4
		    myListBox2.AddRow("")
		    For iColumn As Integer = 0 to myListbox2.ColumnCount - 1
		      strWidths.Append "40"
		      if iRow = 0 Then
		        myListBox2.celltag(myListbox2.LastIndex, iColumn) = 0
		      else
		        myListBox2.celltag(myListbox2.LastIndex, iColumn) = 1
		      end if
		    Next
		    Dim strWidthString As String = Join(strWidths,",")
		    myListBox1.ColumnWidths = strWidthString
		    myListBox2.ColumnWidths = strWidthString
		  Next
		  
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub SetAbacusValues(topListBox As myListBox, bottomListBox As myListBox, iColumn As Integer, iValue As Integer)
		  //Set Values
		  Dim iTop As Integer
		  Dim iBottom As Integer
		  
		  ColumnValues(iColumn) = iValue
		  
		  If ColumnValues(iColumn) >= 5 Then
		    iTop = 1
		    iBottom = ColumnValues(iColumn) - 5
		  Else
		    iTop = 0
		    iBottom = ColumnValues(iColumn)
		  End If
		  
		  ColumnValues(iColumn) = iValue
		  ColumnTopValues(iColumn) = iTop
		  ColumnBottomValues(iColumn) = iBottom
		  
		  topListBox.celltag(0,iColumn) = if(iTop=0,1,0)
		  topListBox.celltag(1,iColumn) = if(iTop=1,1,0)
		  
		  For iRow As Integer = 0 to iBottom - 1
		    bottomListBox.celltag(iRow,iColumn) = 1
		  Next
		  
		  If iBottom <= 4 Then
		    bottomListBox.celltag(iBottom,iColumn) = 0
		  End If
		  
		  For iRow As Integer= (iBottom + 1) to 4
		    bottomListBox.celltag(iRow, iColumn) = 1
		  Next
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		ColumnBottomValues() As Integer
	#tag EndProperty

