#tag Events myListbox2
	#tag Event
		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean
		  #Pragma Unused x
		  #Pragma Unused y
		  Dim iCellValue As Integer = me.CellTag(row,column).IntegerValue
		  
		  if iCellValue=0 Then
		    //do nothing
		  else
		    Dim iColumnValue As Integer = ColumnValues(column)
		    Dim iValue As Integer
		    //we only need the part at the bottom
		    //so subtract 5 if the number in the column is 5 or higher
		    if iColumnValue >= 5 then
		      iValue = iColumnValue - 5
		    else
		      iValue = iColumnValue
		    end if
		    if Row < iValue then
		      //we clicked a value smaller than the current number
		      //so we are subtracting in this column
		      iValue = iValue - (iValue - row)
		    else
		      //we clicked a value higher than the current number
		      //so we are adding in this column
		      iValue = row
		    end if
		    SetAbacusValues(myListBox1,myListBox2,column,iValue+(5*ColumnTopValues(column)))
		  end if
		  
		  
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events myListbox1
	#tag Event
		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean
		  #Pragma Unused x
		  #Pragma Unused y
		  Dim iCellValue As Integer = me.celltag(row,column).IntegerValue
		  if iCellValue=0 Then
		    //do nothing
		  else
		    if row=0 then
		      //we are in the top row, so value is 0
		      //switch, so add 5 to column
		      me.celltag(row,column)=0
		      me.celltag(row+1,column)=1
		      ColumnValues(column)=ColumnValues(column)+5
		      ColumnTopValues(column) = 1
		    else
		      //we are in the bottow row, so value is 5
		      //switch, so subtract 5 from column
		      me.celltag(row,column)=0
		      me.celltag(row-1,column)=1
		      ColumnValues(column)=ColumnValues(column)-5
		      ColumnTopValues(column) = 0
		    end if
		  end if
		  
		End Function
	#tag EndEvent
#tag EndEvents
